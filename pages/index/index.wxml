<!--index.wxml-->
<swiper indicator-dots="{{indicatorDots}}" height="{{h}}"  bindload="imagebindload"  style="height:{{imageheight}}px"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}" wx:key="*this">
    <swiper-item>
      <image src="{{item.url}}" class="slide-image" data-index="{{index}}" bindload="imagebindload" style="width:{{imageWidth}}px;height:{{imageheight}}px"/>
    </swiper-item>
  </block>
</swiper>
